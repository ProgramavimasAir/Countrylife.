			{
				format(String, sizeof(String), "{ffffff}%s {FFAF00}(%d) VIP: {ffffff}%s", GetPlayerNameEx(playerid), playerid, text);
			}
			else
			{
				format(String, sizeof(String), "{ffffff}%s {FFAF00}(%d) VIP: {ffffff}%s", GetPlayerNameEx(playerid), playerid, text);
