COMMAND:parduotifdeze(playerid, params[])
{
	new giveplayerid, DezeCount, DezeKaina, Float: P_Pos[3], String[216];
	
	if(PI[playerid][specialybe] != 4) return 0;

	if(PI[playerid][tdeze] < 1) return SendClientMessage(playerid, -1, "{ffffff} Jus neturite fix dëþiø!");

	if(sscanf(params, "udd", giveplayerid, DezeCount, DezeKaina)) return SendClientMessage(playerid, -1, ""COL_USAGE"[NAUDOJIMAS]: {FFAF00}/parduotifdeze <vardas_pavardë/id> <kiekis vnt> <vnt kaina>");

	if(DezeCount > PI[playerid][tdeze]) return SendClientMessage(playerid, -1, "{ffffff} Jus neturite tiek dëþiø!");

	if(giveplayerid == INVALID_PLAYER_ID) return SendClientMessage(playerid, -1, "{ffffff} Tokio þaidëjo nëra!");

	if(giveplayerid == playerid) return SendClientMessage(playerid, -1, "{ffffff} Ant savæs ðios komandos panaudoti negalite!");

	if(GetPlayerScore(playerid) < NAUJOKO_EXP) return SendClientMessage(playerid, -1, "{ffffff} Ðià komandà jums galima naudoti tik nuo {FFAF00}"#NAUJOKO_EXP"{ffffff} EXP!");

	if(GetPlayerScore(giveplayerid) < NAUJOKO_EXP) return SendClientMessage(playerid, -1, "{ffffff} Ant naujoko ðios komandos panaudoti negalite!");

	if(DezeCount < 1 || DezeKaina < 1) return SendClientMessage(playerid, -1, ""COL_USAGE"[NAUDOJIMAS]: {FFAF00}/parduotifdeze <vardas_pavardë/id> <kiekis vnt> <vnt kaina>");

	GetPlayerPos(giveplayerid, P_Pos[0], P_Pos[1], P_Pos[2]);

	if(!IsPlayerInRangeOfPoint(playerid, 10.0, P_Pos[0], P_Pos[1], P_Pos[2])) return SendClientMessage(playerid, -1, "{ffffff} Þaidëjas per toli nuo jûsø!");

	PI[giveplayerid][rkiekis][0] = DezeCount;
	PI[giveplayerid][rkaina][0]  = DezeCount * DezeKaina;
	ChoosedPlayer[giveplayerid]        = playerid;

	format(String, sizeof(String), "{ffffff}Þaidëjas {ffffff} %s {ffffff}siûlo jums {FFAF00}%s vnt {ffffff}fix dëþes uþ {FFAF00}%seur {ffffff}({FFAF00}1 vnt/%seur{ffffff}).\n \n{ffffff}Ar pirksite?", GetPlayerNameEx(playerid), konvertuoti_pinigus(DezeCount), konvertuoti_pinigus(DezeCount * DezeKaina), konvertuoti_pinigus(DezeKaina));
	ShowPlayerDialog(giveplayerid, 7347, DIALOG_STYLE_MSGBOX, "f dëþës pirkimas", String, "Taip", "Ne");

	format(String, sizeof(String), "{ffffff} [INFO]: {ffffff}Pasiûlëte þaidëjui {ffffff} %s {FFAF00}%s vnt {ffffff}fix dëþes uþ {FFAF00}%seur{ffffff}!", GetPlayerNameEx(giveplayerid), konvertuoti_pinigus(DezeCount), konvertuoti_pinigus(DezeCount * DezeKaina));
	SendClientMessage(playerid, -1, String);

	return 1;
}

