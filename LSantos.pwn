stock MakeToMeMinxMinyMaxxMaxy(Float:x1, Float:x2, Float:x3, Float:x4, Float:y1, Float:y2, Float:y3, Float:y4)
{
	new a, Float:C[4], Float:X[4], Float:Y[4];
	X[0] = x1; Y[0] = y1;
	X[1] = x2; Y[1] = y2;
	X[2] = x3; Y[2] = y3;
	X[3] = x4; Y[3] = y4;
	for(a = 0; a < 4; a++)
	{
		if(a == 0)
		{
			C[0] = X[a]; C[2] = Y[a];
			C[1] = X[a]; C[3] = Y[a];
			continue;
		}
		if(C[0] > X[a]) C[0] = X[a];
		if(C[1] < X[a]) C[1] = X[a];
		if(C[2] > Y[a]) C[2] = Y[a];
		if(C[3] < Y[a]) C[3] = Y[a];
	}
	printf("min_X: %.4f, min_Y: %.4f, max_X: %.4f, max_Y: %.4f", C[0], C[2], C[1], C[3]);
	return 1;
}

