COMMAND:rrpriekabas(playerid)
{
	if(PI[playerid][plead] < 5 && PI[playerid][pavdrk] < 5 && PI[playerid][AdminLevel] < LEVEL_OWNER) return 0;

	new v, String[216];

	for(v = 0; v < MAX_VEHICLES; v++)
	{
		if(GetVehicleModel(v) < 400) 	continue;

		if(VehicleInfo[v][vOwned]) 		continue;

		if(IsPlayersInVehicle(v)) 		continue;

		if(v >= FuristuPriekaba[0] && v <= FuristuPriekaba[8])
		{
			new bool:yra_priekaba = false;
			for(new v2;v2<MAX_VEHICLES;v2++){
				if(GetVehicleTrailer(v2) == v) {
					yra_priekaba = true;
					break;
				}
			}
			if(!yra_priekaba)
				SetVehicleToRespawn(v);
		}
	}

	format(String, sizeof(String), "{ffffff}  %s: {ffffff}Atstaèiau visas priekabas!", GetPlayerNameEx(playerid));
	SendMessageToGdyleriai(-1, String);

	return 1;
}

