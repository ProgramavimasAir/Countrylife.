COMMAND:direktoriai(playerid, params[])
{
	mysql_query("SELECT Vardas,Lead, Prazaide FROM players WHERE Lead > '0'");
	mysql_store_result( );
	new dVardas[MAX_PLAYER_NAME], x, resultline[50], str[130], fstr[1600], dKoks[60], kLead, dStatus[30], prazaides;
	while(mysql_fetch_row_format(resultline))
	{
		sscanf(resultline,"p<|>s[24]dd", dVardas, kLead, prazaides);

		x++;

		if(kLead > 0)
		{
			if(kLead == 1) 			dKoks = "Policijos direktorius (-ë)";
			else if(kLead == 2) 	dKoks = "Medikø direktorius (-ë)";
			else if(kLead == 3)	 	dKoks = "Taxistø direktorius (-ë)";
			else if(kLead == 4) 	dKoks = "Mechanikø direktorius (-ë)";
			else if(kLead == 5) 	dKoks = "Furistø direktorius (ë)";
			else if(kLead == 6) 	dKoks = "The Ballas Gaujos Vadas (-ë)";
			else if(kLead == 7) 	dKoks = "GroveStreet Gaujos Vadas (-ë)";

			if(GetPlayeridMid(dVardas) != INVALID_PLAYER_ID) { dStatus = "{FFAF00}ONLINE{ffffff}"; } else { dStatus = "{ffffff} OFFLINE{ffffff}"; }

			format(str, sizeof(str), "{ffffff}%d. %s - %s | Statusas: %s | {ffffff}Ðiandien praþaidë: %d min\n", x, dKoks, dVardas, dStatus, prazaides);
			strcat(fstr, str);
		}
	}

	ShowPlayerDialog(playerid, 1114, DIALOG_STYLE_MSGBOX, "Direktoriai", fstr, "Uþdaryti", "");

	mysql_free_result();

	return 1;
}

