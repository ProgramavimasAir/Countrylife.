		else
		{
			TogglePlayerSpectating(playerid, 1);
			PlayerSpectatePlayer(playerid, giveplayerid);
		}

		format(String, sizeof(String), "{ffffff} Priþiûrite þaidëjà {ffffff} %s{ffffff}!", GetPlayerNameEx(giveplayerid));
