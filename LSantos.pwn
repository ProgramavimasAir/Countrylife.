COMMAND:sunaikinti(playerid)
{
	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, -1, "{ffffff} Turite bûti transp. priemonëje!");

	if(GetPlayerState(playerid) == PLAYER_STATE_PASSENGER) return SendClientMessage ( playerid, COLOR_RED, "{ffffff} Maðinoje ðios komandos naudoti negali!");

	new vehicleid = GetPlayerVehicleID(playerid);
	new model     = GetVehicleModel(vehicleid);

	VehicleInfo[vehicleid][Rida] = 0.0;

	switch(PI[playerid][specialybe])
	{
		case 0: return 0;
		case 1: if(vehicleid >= PolicijosMasina[0] && vehicleid <= PolicijosMasina[26]) SetVehicleToRespawn(GetPlayerVehicleID(playerid));
		case 2: if(vehicleid >= MedikuMasina[0] && vehicleid <= MedikuMasina[13]) SetVehicleToRespawn(GetPlayerVehicleID(playerid));
		case 3: if(vehicleid >= TaxiMasina[0] && vehicleid <= TaxiMasina[11]) SetVehicleToRespawn(GetPlayerVehicleID(playerid));
		case 4: if(vehicleid >= mechanikumasina[0] && vehicleid <= mechanikumasina[13]) SetVehicleToRespawn(GetPlayerVehicleID(playerid));
		case 5: if(vehicleid >= KurjeriuMasina[0] && vehicleid <= KurjeriuMasina[8]) SetVehicleToRespawn(GetPlayerVehicleID(playerid));
		case 6: if(vehicleid == lucheese_car[0] || model == 566 || model == 517) SetVehicleToRespawn(GetPlayerVehicleID(playerid));
		case 7: if(vehicleid == triads_car[0] || model == 492 || model == 567) SetVehicleToRespawn(GetPlayerVehicleID(playerid));

