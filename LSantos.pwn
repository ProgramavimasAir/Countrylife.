stock UpdateSpeedo(playerid)
{
	if(!IsPlayerInAnyVehicle(playerid)) return 1;

	new vehicleid = GetPlayerVehicleID(playerid);

	new msg[128];

	new Float:vHP;

	new pSpeed = GetPlayerSpeed(playerid);

	GetVehicleHealth(vehicleid, vHP);

	format(msg, 40, "%s", VehicleNames[GetVehicleModel(vehicleid) - 400]);
	TextDrawSetString(SpeedoText[playerid][1], msg);

	if(vHP < 350.0)
	{
		format(msg, 30, "Bukle: ~r~%.1f", vHP);
		TextDrawSetString(SpeedoText[playerid][9], msg);
	}
	else if(vHP > 350.0 && vHP < 600.0)
	{
		format(msg, 30, "Zala: ~y~%.1f", vHP);
		TextDrawSetString(SpeedoText[playerid][9], msg);
	}
	else if(vHP > 600.0)
	{
		format(msg, 30, "Zala: ~g~%.1f", vHP);
		TextDrawSetString(SpeedoText[playerid][9], msg);
	}

	format(msg, 33, "Rida: ~g~%.2f KM", VehicleInfo[vehicleid][Rida]);
	TextDrawSetString(SpeedoText[playerid][11], msg);

	if(pSpeed == 0)
	{
		format(msg, 31, "00%d", pSpeed);
		TextDrawSetString(SpeedoText[playerid][7], msg);
	}
	else if(pSpeed > 0 && pSpeed < 10)
	{
		format(msg, 31, "~g~00%d", pSpeed);
		TextDrawSetString(SpeedoText[playerid][7], msg);
	}
	else if(pSpeed > 10 && pSpeed <= 90)
	{
		format(msg, 31, "~g~0%d", pSpeed);
		TextDrawSetString(SpeedoText[playerid][7], msg);
	}
	else if(pSpeed > 90 && pSpeed < 100)
	{
		format(msg, 31, "~r~0%d", pSpeed);
		TextDrawSetString(SpeedoText[playerid][7], msg);
	}
	else if(pSpeed >= 100)
	{
		format(msg, 31, "~r~%d", pSpeed);
		TextDrawSetString(SpeedoText[playerid][7], msg);
	}
	
	if(VehicleInfo[vehicleid][gas] < 10.0)
	{
		format(msg, 25, "Kuras:~r~ %.2f~w~ L", VehicleInfo[vehicleid][gas]);
		TextDrawSetString(SpeedoText[playerid][10], msg);
	}
	else
	{
		format(msg, 25, "Kuras:~g~ %.2f~w~ L", VehicleInfo[vehicleid][gas]);
		TextDrawSetString(SpeedoText[playerid][10], msg);
	}

	return 0;
}

stock IsABike(vehicleid)
{
	switch(GetVehicleModel(vehicleid))
	{
		case 509, 481, 510: return true;
	}

	return false;
}

stock IsAMoto(vehicleid)
{
	switch(GetVehicleModel(vehicleid))
	{
		case 462, 448, 581, 522, 461, 521, 523, 463, 468, 471, 478: return true;
	}

	return false;
}

stock GetPlayerSpeed(playerid)
{
	new Float:ST[4];
	if(IsPlayerInAnyVehicle(playerid))
	GetVehicleVelocity(GetPlayerVehicleID(playerid), ST[0], ST[1], ST[2]);
	else GetPlayerVelocity(playerid, ST[0], ST[1], ST[2]);
	ST[3] = floatsqroot(floatpower(floatabs(ST[0]), 2.0) + floatpower(floatabs(ST[1]), 2.0) + floatpower(floatabs(ST[2]), 2.0)) * 170.3; // 254.3
	return floatround(ST[3]);
}

stock SetPlayerSpeedCap(playerid, Float:value)
{
   if(0 <= playerid < sizeof(g_fSpeedCap)) g_fSpeedCap[playerid] = value;
}

