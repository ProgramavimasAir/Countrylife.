stock CountGangMembersInterior(gangid, interiorid)
{
	new count;

	foreach(Player, p)
	{
		if(PI[p][gMember] == gangid)
		{
			if(GetPlayerInterior(p) == interiorid)
			{
				count ++;
			}
		}
	}

	return count;
}
stock replaceSwear(playerid,String[], badWord[],replace = '*')
{
	while((ANTIKEISKM[ playerid ] = strfind(String, badWord, true)) != -1)
	{
		for(ANTIKEISKM2[ playerid ] = (ANTIKEISKM[ playerid ] + strlen(badWord)); ANTIKEISKM[ playerid ] != ANTIKEISKM2[ playerid ]; ++ANTIKEISKM[ playerid ])
		{
			String[ANTIKEISKM[ playerid ]] = replace;
		}
	}
