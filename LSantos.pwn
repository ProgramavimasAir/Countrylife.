			SendMessageToCops(-1, String);
		}

		case 2:
		{
			if(PI[giveplayerid][Mteises] == false) return SendClientMessage(playerid, -1, "{ffffff} Þaidëjas neturi ðios kategorijos teisiø!");

			PI[giveplayerid][Mteises] = false;

			format(String, sizeof(String), "{ffffff} [INFO]: {ffffff} %s {ffffff}atëme ið jûsø motociklo vairavimo teises!", GetPlayerNameEx(playerid));
			SendClientMessage(giveplayerid, -1, String);

			format(String, sizeof(String), "{ffffff}  %s: {ffffff}Atëmiau motociklo vairavimo teises ið {ffffff} %s{ffffff}.", GetPlayerNameEx(playerid), GetPlayerNameEx(giveplayerid));
			SendMessageToCops(-1, String);
		}

		case 3:
		{
			SendClientMessage(playerid, -1, "{ffffff} Þaidëjas neturi ginklø licenzijos!");

			format(String, sizeof(String), "{ffffff} [INFO]: {ffffff} %s {ffffff}atëme ið jûsø ginklø licenzijà!", GetPlayerNameEx(playerid));
			SendClientMessage(giveplayerid, -1, String);

			format(String, sizeof(String), "{ffffff}  %s: {ffffff}Atëmiau ginklø licenzijà ið {ffffff} %s{ffffff}.", GetPlayerNameEx(playerid), GetPlayerNameEx(giveplayerid));
			SendMessageToCops(-1, String);
		}

