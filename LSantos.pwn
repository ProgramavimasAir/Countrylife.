COMMAND:pverslas(playerid, params[])
{
	new Float: ProP_Pos[3], PropTitle[64], String[300];

	if(PI[playerid][AdminLevel] < LEVEL_OWNER) return 0;

	if(sscanf(params, "s[64]", PropTitle)) return SendClientMessage(playerid, -1, "Komandos naudojimas: {ffffff} /cproperty <pavadinimas>");

	for(new Property; Property < MAX_PROPERTYS; Property ++)
	{
		format(String, sizeof(String), "SELECT * FROM `verslai` WHERE `ID` = '%d'", Property);
		mysql_query(String);

		mysql_store_result();

		if(!mysql_num_rows())
		{
		    GetPlayerPos(playerid, ProP_Pos[0], ProP_Pos[1], ProP_Pos[2]);

			format(PropertyInfo[Property][pOwner], MAX_PLAYER_NAME, "Nera");

			strdel(PropertyInfo[Property][pTitle], 0,         64);
			strcat(PropertyInfo[Property][pTitle], PropTitle, 64);

			PropertyInfo[Property][pCreated] 	= true;
			PropertyInfo[Property][pBuyed] 		= false;
	 		PropertyInfo[Property][pPrice] 		= 25000;
			PropertyInfo[Property][pProfit] 	= 0;
			//PropertyInfo[Property][pEarning] 	= PropEarning;
			PropertyInfo[Property][pEarning] 	= floatround(PropertyInfo[Property][pPrice] / 5000);
			PropertyInfo[Property][pPos_X] 		= ProP_Pos[0];
			PropertyInfo[Property][pPos_Y] 		= ProP_Pos[1];
			PropertyInfo[Property][pPos_Z] 		= ProP_Pos[2];

			format(String, sizeof(String), "{ffffff}Verslas: {ffffff}%s\n \n{ffffff}Unikalus ID: {ffffff}%d\n{ffffff}Savininkas: {ffffff}%s\n{ffffff}Kaina: {ffffff}%seur\n{ffffff}Uþdarbis: {ffffff}$%d/min\n{ffffff}Norëdami valdyti verslà spauskite {99cc00}ALT",
			PropertyInfo[Property][pTitle], Property, PropertyInfo[Property][pOwner], konvertuoti_pinigus(PropertyInfo[Property][pPrice]), PropertyInfo[Property][pEarning]);

			PropertyInfo[Property][pText] 		= Create3DTextLabel (String, -1, PropertyInfo[Property][pPos_X], PropertyInfo[Property][pPos_Y], PropertyInfo[Property][pPos_Z], 10, 0, 0);
			PropertyInfo[Property][pPickup] 	= CreatePickup		(19132,	  1, PropertyInfo[Property][pPos_X], PropertyInfo[Property][pPos_Y], PropertyInfo[Property][pPos_Z], 0);

			format(String, sizeof(String), "INSERT INTO `verslai` (ID) VALUES ('%d')", Property);
			mysql_query(String);

			SaveProperty(Property);

			SendClientMessage(playerid, -1, "Sëkmingai sukurëte verslà.");

			return 1;
		}

		mysql_free_result();
	 }

	return SendClientMessage(playerid, -1, "Jus jau pasiekëte leistinà verslø limità.");
}
