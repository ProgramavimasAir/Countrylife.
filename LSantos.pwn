	if(giveplayerid == playerid) return SendClientMessage(playerid, -1, ""COL_ERROR" {ffffff}Ant savæs ðios komandos panaudoti negalite!");

	if(AHS[giveplayerid][AskedForHelp] == false) return SendClientMessage(playerid, -1, ""COL_ERROR" {ffffff}Þaidëjas nepraðë pagalbos arba jam jau padëjo kitas administratorius!");

	format(String, sizeof(String), "{ffffff} Administratorius {FFAF00}%s{ffffff} atsakë á þaidëjo {ffffff} %s{ffffff} pateiktà klausimà:", GetPlayerNameEx(playerid), GetPlayerNameEx(giveplayerid));
	SendMessageToAdmins(String);
	format(String, sizeof(String), "{ffffff} {FFF064}%s", HelpText);
	SendMessageToAdmins(String);

	format(String, sizeof(String), "{ffffff} {ffffff}Administratorius {FFAF00}%s{ffffff} atsakë á jûsø pateiktà klausimà:", GetPlayerNameEx(playerid));
	SendClientMessage(giveplayerid, -1, String);
	format(String, sizeof(String), "{ffffff} {FFF064}%s", HelpText);
	SendClientMessage(giveplayerid, -1, String);

	AHS[playerid][HelpsInAll] 		++;

	format(String, sizeof(String), "UPDATE `players` SET `HelpsInAll` = '%d' WHERE `Vardas` = '%s'",
    AHS[playerid][HelpsInAll], GetPlayerNameEx(playerid));
	mysql_query(String);

	AHS[giveplayerid][AskedForHelp] = false;

	return 1;
}

COMMAND:admin(playerid, params[])
{
	new String[1500], Stringas[1500], AdminCount, KoksLygis[40], Status[30];

	if(sscanf(params, "s[128]", params))
	{
		for(new i = 0; i <= MAX_PLAYERS; i++)
		{
			if(IsPlayerConnected(i) && PI[i][AdminLevel] > LEVEL_VIP)
			{
				AdminCount ++;

				if(AdminCount == 0) return SendClientMessage(playerid, -1, ""COL_ERROR" {ffffff}Ðiuo metu prisijungusiø administratoriø nëra!");

				if(PI[i][AOD]) 	Status = "{FFAF00}Dirba";
				else 			Status = "{ffffff} Nedirba";

				switch(PI[i][AdminLevel])
				{
					case LEVEL_FIRST: 	KoksLygis = "{FFAF00}I lygio administratorius";
					case LEVEL_SECOND: 	KoksLygis = "{FFAF00}II lygio administratorius";
					case LEVEL_THIRD: 	KoksLygis = "{FFAF00}III lygio administratorius";
					case LEVEL_PAGAL: 	KoksLygis = "{FFAF00}Vyr.Administratorius";
					case LEVEL_OWNER: 	KoksLygis = "{FFAF00}Savininkas";
				}

				format(Stringas, sizeof(Stringas), "{ffffff}%d. {ffffff} %s{ffffff}, pareigos: %s{ffffff}, statusas: %s\n", AdminCount, GetPlayerNameEx(i), KoksLygis, Status);
				strcat(String, Stringas);

				ShowPlayerDialog(playerid, 6956, DIALOG_STYLE_MSGBOX, "Prisijungusi serverio valdþia", String, "Þinutë", "Uþdaryti");
			}
		}

		return 1;
	}
	else
	{
		if(PI[playerid][ParaseAdminams] > gettime()) return SendClientMessage(playerid, -1, ""COL_ERROR" {ffffff}Administratoriams galima raðyti tik kas 30 sekundþiø!");

		if(strlen(params) > 128) return SendClientMessage(playerid, -1, ""COL_ERROR" {ffffff}Tekstas per ilgas!");

		for(new i = 0; i <= MAX_PLAYERS; i++)
		{
			if(IsPlayerConnected(i) && PI[i][AdminLevel] > LEVEL_VIP)
			{
				format(String, sizeof(String), "{ffffff} {FFAF00}%s(%d) administratoriams: {ffffff}%s (/pn %i)", GetPlayerNameEx(playerid), playerid, params, playerid);
				if(PI[i][AOD]) SendClientMessage(i, -1, String);
			}
		}

		if(PI[playerid][AdminLevel] < LEVEL_FIRST)
		{
			format(String, sizeof(String), "{ffffff} {FFAF00}Paraðëte administratoriams: {ffffff}%s", params);
			SendClientMessage(playerid, -1, String);
		}

		PI[playerid][ParaseAdminams] = gettime() + 30;
		AHS[playerid][AskedForHelp] = true;
	}

	return 1;
}

