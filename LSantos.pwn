    if(listid == skinList)
    {
        if(response)
        {
            SendClientMessage(playerid, 0xFF0000FF, "Nusipirkai norimà skinà");
            SetPlayerSkin(playerid, modelid);
            PI[playerid][drabuziai] = modelid;
        }
        else SendClientMessage(playerid, 0xFF0000FF, "Nutraukei skin keitimà.");
        return 1;
    }
    if(listid == GskinList)
    {
        if(response)
        {
            SendClientMessage(playerid, 0xFF0000FF, "Nusipirkai norimà gaujos skinà");
