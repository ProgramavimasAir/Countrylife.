f_public: ZvejybosVarzybos()
{
	new String[216];

	if(!Varzybos)
	{
		Varzybos 		= true;
		VarzybuPrizas 	= 5000 + random(5000);

		format(String, sizeof(String), "{fffff} {ffffff}Prasidëjo {FFAF00}þvejybos varþybos{ffffff}, jas laimëjæs þmogus bus apdovanotas {FFAF00}%s eur{ffffff}!", konvertuoti_pinigus(VarzybuPrizas));
		SendClientMessageToAll(-1, String);
		SendClientMessageToAll(-1, "{ffffff} Varþybos truks {FFAF00}15{ffffff} minuèiø, tad paskubëkite!");

		SetTimer("StabdomVarzybas",	60000 * 15, false);
		//SetTimer("StabdomVarzybas",	30000, false);
	}

	return 1;
}

f_public: StabdomVarzybas()
{
	new String[216];

	if(Varzybos)
	{
		Varzybos = false;

		for(new i = 0; i < MAX_PLAYERS; i++)
		{
		   	if(!IsPlayerConnected(i)) continue;

		   	if(KiekSugavo[i] > KiekSugavo[i])
		   	{
		      	daugiausiaiTuri = i;
		      	kieksugavoa = KiekSugavo[daugiausiaiTuri];
		   	}
		}

	    if(daugiausiaiTuri != INVALID_PLAYER_ID)
	    {
	    	if(kieksugavoa > 0)
	    	{
			    format(String, sizeof(String), "{fffff} {FFAF00}Þvejybos varþybos pasibaigë{ffffff}! Jas laimëjo: {FFAF00}%s{ffffff}!", GetPlayerNameEx(daugiausiaiTuri));
				SendClientMessageToAll(-1, String);

				format(String, sizeof(String), "{fffff} Jis viso sugavo {FFAF00}%s {ffffff}kg þuvø ir laimëjo {FFAF00}%seur{ffffff}!", konvertuoti_pinigus(kieksugavoa), konvertuoti_pinigus(VarzybuPrizas));
				SendClientMessageToAll(-1, String);

				GivePlayerMoneyA(daugiausiaiTuri, VarzybuPrizas);
			}
			else
			{
				SendClientMessageToAll(-1, "{fffff} {FFAF00}Þvejybos varþybos pasibaigë{ffffff}! Jø nelaimëjo {FFAF00}niekas{ffffff}!");
			}
		}
		else
		{
			SendClientMessageToAll(-1, "{fffff} {FFAF00}Þvejybos varþybos pasibaigë{ffffff}! Jø nelaimëjo {FFAF00}niekas{ffffff}!");
		}

		for(new p = 0; p < MAX_PLAYERS; p ++)
	    {
	        KiekSugavo[p] = 0;
	    }
	}

	return 1;
}

