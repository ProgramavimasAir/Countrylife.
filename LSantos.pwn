	}
	ShowPlayerDialog(playerid, 32766, DIALOG_STYLE_TABLIST_HEADERS, "Serverio gaujos", msg, "-", "");

	return 1;
}

CMD:gdel(playerid, params[]) {

	if(PI[playerid][AdminLevel] < LEVEL_PAGAL) return 0;
	if(isnull(params)) return SendClientMessage(playerid, COLOR_RED, "/gdel <gaujos id>");
	new kure = strval(params);
	if(isnull(GangInfo[kure][gTitle])) return SendClientMessage(playerid, COLOR_RED, "Tokios gaujos nëra");

	new Query[128];

	format(Query, sizeof(Query), "SELECT * FROM `players` WHERE `gMember` = '%d'", kure);
    mysql_query(Query);

	mysql_store_result();

	if(mysql_num_rows() != 0)
	{
	    while(mysql_retrieve_row())
	    {
	        format(Query, sizeof(Query), "UPDATE `players` SET `gMember` = '-1' WHERE `gMember` = '%d'", kure);
	        mysql_query(Query);
	    }
	}

	mysql_free_result();

	strdel(GangInfo[kure][gTitle],  0, 24);
	strdel(GangInfo[kure][gLeader], 0, MAX_PLAYER_NAME);

	GangInfo[kure][G_Pos][0] = 0.0000;
	GangInfo[kure][G_Pos][1] = 0.0000;
	GangInfo[kure][G_Pos][2] = 0.0000;

	GangInfo[kure][gChest] 	= -1;
	GangInfo[kure][gPoints] = -1;
	GangInfo[kure][Ginklai1]	= 0;
	GangInfo[kure][Ginklai2]	= 0;
	GangInfo[kure][Ginklai3]	= 0;
	GangInfo[kure][Narkai1]		= 0;
	GangInfo[kure][Narkai2]		= 0;
	GangInfo[kure][Narkai3]		= 0;
	GangInfo[kure][Narkai4]		= 0;

	GangInfo[kure][gTele]		= false;
	GangInfo[kure][gVirve]		= false;
	GangInfo[kure][gIsodinti]	= false;

    DestroyPickup            	(GangInfo[kure][gPickup]);
	DestroyDynamic3DTextLabel 	(GangInfo[kure][gLabel]);

	format(Query, sizeof(Query), "DELETE FROM `gaujuinfo` WHERE `ID` = '%d'", kure);
	mysql_query(Query);

	for(new p = 0; p < MAX_PLAYERS; p ++)
