f_public: MatuojaGreiti()
{
	new pSpeed, String[216];
	for(new playerid; playerid < MAX_PLAYERS; playerid++)
	{
		if(!IsPlayerConnected(playerid) || IsPlayerNPC(playerid)) continue;
		if(IsPlayerInRangeOfPoint(playerid, 20.0, 1365.4255, -1385.9843, 12.6655)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 1334.8326, -1413.9047, 12.6655)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 1319.0132, -1611.6685, 12.5333)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 1290.4181, -1612.4586, 12.5321)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 1437.1984, -1583.5199, 12.5374)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 1457.4650, -1739.6423, 12.5395)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 1480.5110, -1878.9830, 12.5372)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 1880.8424, -1744.9331, 12.5285)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 1954.5717, -1860.7755, 12.5362)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 2088.6963, -1870.2903, 12.5386)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 1648.9779, -1433.3187, 12.5341)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 1587.3197, -1167.3263, 23.0751)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 1112.1112, -1134.5929, 22.8140)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 620.5610, -1380.1067, 12.6867)
		|| IsPlayerInRangeOfPoint(playerid, 20.0, 626.7335, -1737.7792, 12.5516))
		{
			if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
			{
				new vehicleid = GetPlayerVehicleID(playerid);
				if(!IsABike(vehicleid))
				{
					if(!PI[playerid][suautopilotu])
					{
						if(PI[playerid][specialybe] != 1)
						{
							if(PI[playerid][AdminLevel] < LEVEL_OWNER)
							{
								pSpeed = GetPlayerSpeed(playerid);
								if(pSpeed > 105)
								{
								    new time[2];
								    gettime(time[0], time[1], time[1]);
								    if(time[0] >= 0 && time[0] <= 7) return SendClientMessage(playerid, -1, "{E26E67}[MATUOKLIS] {ffffff}Nuo 00:00 iki 07:00 greièio matuokliai neveikia");
									if(GreitiMatuoja[playerid] <= gettime())
									{
										GreitiMatuoja[playerid] = gettime() + 4;

										if(PI[playerid][pinigai] > 299) 	GivePlayerMoneyA(playerid, -500);
										if(PI[playerid][teises]) 			PI[playerid][GreicioBalai] ++;

										format(String, sizeof(String), "{E26E67}[MATUOKLIS] {ffffff}Virðijote leistinà {FFAF00}90 {ffffff}KM/H greitá ({FFAF00}%d {ffffff}KM/H). Bauda: {FFAF00}500eur{ffffff}!", pSpeed);
										SendClientMessage(playerid, -1, String);

										format(String, sizeof(String), "{E26E67}[INFO] {ffffff}Gavote {FFAF00}%d/10 {ffffff}vairavimo baudos balà (-us)!", PI[playerid][GreicioBalai]);
										if(PI[playerid][teises]) SendClientMessage(playerid, -1, String);

										FadeScreenForPlayer(playerid, FADE_OUT, 0xFFFFFFFF);

										if(PI[playerid][GreicioBalai] > 9 && PI[playerid][teises])
										{
											SendClientMessage(playerid, -1, "{E26E67}[INFO] {ffffff}Praradote vairavimo teisës, nes surinkote {FFAF00}10 {ffffff}vairavimo baudos balø!");

											PI[playerid][teises] 		= false;
											PI[playerid][pteises]       = false;
											PI[playerid][GreicioBalai] 	= 0;
										}
										else if(PI[playerid][pteises] == true)
										{
										if(PI[playerid][pinigai] > 500) 	GivePlayerMoneyA(playerid, -1000);
										
										format(String, sizeof(String), "{E26E67}[MATUOKLIS] {ffffff}Kadangi neturite teisiø, bet ir toliau nusiþengiate, jûsø bauda padidëjo iki {FFAF00}1.000eur{ffffff}!", pSpeed);
										SendClientMessage(playerid, -1, String);
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

	return 1;
}

