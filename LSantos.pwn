			new Float:x, Float:y, Float:z;
			GetPlayerPos(damagedid, x, y, z);
			ClearAnimations(damagedid);
			ApplyAnimation(damagedid, "PED","FLOOR_hit_f", 4.1, 1, 1, 1, 1, 10000, 1);

			Spark[damagedid] = CreateObject(TAZE_SPARK, x, y, z-3, 0, 0, 0);
			SetTimerEx("DestroySpark", TAZE_DESTROY, 0, "i", damagedid);
			SetTimerEx("TazedRemove", TAZE_TIMER, 0, "i", damagedid);
			Tazed[damagedid] = true;
			PI[playerid][NaudojoTazeri] = gettime() + 20;
			if(losehp) {} else SetPlayerHealthEx(damagedid, health+amount);

			new String[128];

			format(String, sizeof(String), "{ffffff} Jus su tazeriu paðovë {FFAF00}%s{ffffff}!", GetPlayerNameEx(playerid));
			SendClientMessage(damagedid, -1, String);
		}
	}
	if(GetPlayerScore(damagedid) < 1)
	{
	    new Float:hp, Float:hpa;
	    GetPlayerHealth(damagedid, hp);
	    GetPlayerHealth(playerid, hpa);
		SetPlayerHealth(damagedid, hp + 10.0);
		SetPlayerHealth(playerid, hpa- 10.0);
		SendClientMessage(playerid, -1, "{ffffff}Suþeidei naujokà, todël netekai {FFAF00}10 HP");
	}
	return 1;
}

