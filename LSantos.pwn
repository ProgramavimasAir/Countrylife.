public OnPlayerDeath(playerid, killerid, reason)
{
	PI[playerid][pHealthCheck] 	= -5;
	AntiF4[playerid] 					= 1;
	NesaPica[playerid] 					= 0;
	PicaCP[playerid] 					= false;
	TaksiCP[playerid]                   = false;

	KillTimer(nuomaTimer[playerid]);

	for(new s = 0; s < 12; s++) TextDrawHideForPlayer(playerid, SpeedoText[playerid][s]);

	DestroyProgressBar(VandensBar[playerid]);
	VandensBar[playerid] = INVALID_BAR_ID;

	TextDrawHideForPlayer(playerid, Lasas[playerid]);

	ExitFromCarShop(playerid);

	MoveToHospital(playerid);

	if(TaxiInfo[playerid][imti]) TaxiInfo[playerid][imti] = false;

	if(PI[playerid][DirbaTaksi])
    {
    	new vehicleid = PI[playerid][masina];

		PI[playerid][DirbaTaksi] 	= false;
		ArTaksi[vehicleid]			= false;
    }

	if(PI[playerid][lektuvas] != -1)
	{
		SetPlayerVirtualWorld(playerid, 0);
		SetPlayerInterior(playerid, 0);
		PI[playerid][lektuvas] = -1;
	}

	if(killerid == INVALID_PLAYER_ID) return 1;
	else
	{
		if(PI[killerid][gMember] != -1 && PI[playerid][gMember] != -1)
		{
			if(PI[playerid][gMember] != PI[killerid][gMember])
			{
				new String[216];

				GangInfo[PI[killerid][gMember]][gPoints] ++;
				GangInfo[PI[playerid][gMember]][gPoints] --;

				format(String, sizeof(String), "{ffffff} [GAUJA]: {ffffff}Nuþudëte kitos gaujos nará, jums prisidëjo +1 taðkas. Dabar turite: {FFAF00}%d{ffffff} gaujos taðkø!", GangInfo[PI[killerid][gMember]][gPoints]);
				SendClientMessage(killerid, -1, String);

				format(String, sizeof(String), "{ffffff} [GAUJA]: {ffffff}Jus nuþudë kitos gaujos narys, jums nuimtas 1 taðkas. Dabar turite: {FFAF00}%d{ffffff} gaujos taðkø!", GangInfo[PI[playerid][gMember]][gPoints]);
				SendClientMessage(playerid, -1, String);

				SaveGang(PI[killerid][gMember]);
				SaveGang(PI[playerid][gMember]);
			}
		}
	}

	if(PI[playerid][Nuskriaude] != killerid)
	{
		PI[playerid][FakeKill] ++;
		PI[playerid][Nuskriaude] = INVALID_PLAYER_ID;
		if(PI[playerid][FakeKill] > 2)
		{
			Kick(playerid);
		}
		return 1;
	}
    if(IsPlayerInRangeOfPoint( playerid, 60.0, 2578.1941,2829.9695,10.8203 ))
	{
	   SetPlayerWantedLevel(killerid, GetPlayerWantedLevel(killerid) + 0);
	   PI[killerid][DMTASKAI] +=1;
	   GivePlayerMoneyA(playerid, 5);
	   SendClientMessage(killerid, BLUE, "{ffffff}[DM]: {ffffff}Gavai {FFAF00}+1{ffffff} DM taðkà");
	   return 1;
	}
	if(!PlayerInZone(playerid, 2816.5810, -2555.4128, 2862.4848, -2324.5683))
	{
		if(PI[killerid][specialybe] != 1)
		{
			//SetPlayerWantedLevel(killerid, GetPlayerWantedLevel(killerid) + 3);
			givestar(killerid, 3);
		}
	}

	if(!PlayerInZone(playerid, 2816.5810, -2555.4128, 2862.4848, -2324.5683))
	{
		if((PI[killerid][specialybe] == 1) && PI[playerid][itarimas] > 0)
		{
			if(PI[playerid][specialybe] != 1)
			{
				JailPlayer(playerid, (PI[playerid][itarimas] * 60));
				return 1;
			}
		}
	}

	if((PI[killerid][specialybe] == 1) && PI[playerid][itarimas] < 0)
	{
		SendClientMessage(killerid, BLUE, "* Nuðovei nekaltà þmogø.");
	}

	NeedCheckTuningAB [ playerid ] = 5;
	update_alkis(playerid);

	return 1;
}

