COMMAND:ant(playerid, params[])
{
	if(PI[playerid][specialybe] != 1) return 0;

	if(GetPlayerState(playerid) == PLAYER_STATE_PASSENGER) return SendClientMessage ( playerid, COLOR_RED, "{ffffff} {ffffff} {ffffff}Maðinoje ðios komandos naudoti negali!");

	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER) return SendClientMessage ( playerid, COLOR_RED, "{ffffff} {ffffff} {ffffff}Maðinoje ðios komandos naudoti negali!");

	new giveplayerid, Float: P_Pos[3], String[216];

	if(!PI[playerid][uniformd]) return SendClientMessage(playerid, -1, "{ffffff} Jus esate be darbiniø drabuþiø!");

	if(sscanf(params, "u", giveplayerid)) return SendClientMessage(playerid, -1, "{ffffff} {FFAF00}/ant <vardas_pavardë/id>");

	if(giveplayerid == INVALID_PLAYER_ID) return SendClientMessage(playerid, -1, "{ffffff} Tokio þaidëjo nëra!");

	if(giveplayerid == playerid) return SendClientMessage(playerid, -1, "{ffffff} Ant savæs ðios komandos panaudoti negalite!");

	if(GetPlayerScore(giveplayerid) < NAUJOKO_EXP) return SendClientMessage(playerid, -1, "{ffffff} Ant naujoko ðios komandos panaudoti negalite!");

	if(PI[playerid][banded]) return SendClientMessage(playerid, -1, "{ffffff} Jus esate suriðtas!");

	GetPlayerPos(giveplayerid, P_Pos[0], P_Pos[1], P_Pos[2]);

	if(!IsPlayerInRangeOfPoint(playerid, 7.0, P_Pos[0], P_Pos[1], P_Pos[2])) return SendClientMessage(playerid, -1, "{ffffff} Þaidëjas per toli nuo jûsø!");

	if(PI[giveplayerid][cuffed])
	{
		Surakinti(giveplayerid, true);

		format(String, sizeof(String), "{ffffff} [INFO]: {ffffff} %s {ffffff}nuëme jums antrankius!", GetPlayerNameEx(playerid));
		SendClientMessage(giveplayerid, -1, String);

		format(String, sizeof(String), "{ffffff} [DARBAS]: {ffffff}Nuëmete antrankius þaidëjui {ffffff} %s{ffffff}!", GetPlayerNameEx(giveplayerid));
		SendClientMessage(playerid, -1, String);
		
		format(String, sizeof(String), "{ffffff} [RACIJA] Policinimkas {ffffff} %s: {ffffff}Nuëmë antrankius: {ffffff} %s{ffffff}.", GetPlayerNameEx(playerid), GetPlayerNameEx(giveplayerid));
	    SendMessageToCops(-1, String);

		format(String, sizeof(String), "UPDATE `players` SET `Cuffed` = '0' WHERE `Vardas` = '%s'", GetPlayerNameEx(giveplayerid));
		mysql_query(String);
	}
	else
	{
		Surakinti(giveplayerid, false);

		format(String, sizeof(String), "{ffffff} [INFO]: {ffffff} %s {ffffff}uþdëjo jums antrankius!", GetPlayerNameEx(playerid));
		SendClientMessage(giveplayerid, -1, String);

		format(String, sizeof(String), "{ffffff} [DARBAS]: {ffffff}Uþdëjote antrankius þaidëjui {ffffff} %s{ffffff}!", GetPlayerNameEx(giveplayerid));
		SendClientMessage(playerid, -1, String);
		
		format(String, sizeof(String), "{ffffff} [RACIJA] Policinimkas {ffffff} %s: {ffffff}Uþdëjo antrankius: {ffffff} %s{ffffff}.", GetPlayerNameEx(playerid), GetPlayerNameEx(giveplayerid));
	    SendMessageToCops(-1, String);

		format(String, sizeof(String), "UPDATE `players` SET `Cuffed` = '1' WHERE `Vardas` = '%s'", GetPlayerNameEx(giveplayerid));
		mysql_query(String);
	}

	return 1;
}

COMMAND:atim(playerid, params[])
{
