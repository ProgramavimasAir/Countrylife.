			DestroyObject(deerId);
			deerId=0;
			new msg[120];
			new price=100+random(250);
			new xp=random(20)+10;
			format(msg,sizeof(msg),"{ffffff} [ELNIAS]{FFAF00} %s {ffffff}Nuðovë elnià ir gavo{FFAF00} %i {ffffff}EUR ir{FFAF00} %i {ffffff}XP",GetPlayerNameEx(playerid),price,xp);
			SendClientMessageToAll(-1,msg);
			GivePlayerMoneyA(playerid, price);
    	    GivePlayerExperience (playerid, xp);
			SetTimer("SpawninamElnia", 1000*60*10, true);
			for(new i = 0; i < MAX_PLAYERS; i++)
	        {
                if(IsPlayerConnected(i))
		        {
