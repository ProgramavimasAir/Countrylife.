f_public: kraunamprekesEx(playerid, modid)
{
	new String[128];

	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, -1, "{ffffff} Turite bûti Vairuotojo vietoje!");

	new vehicleid = GetPlayerVehicleID(playerid);

	if(!(KurjeriuMasina[0] <= vehicleid <= KurjeriuMasina[8])) return SendClientMessage(playerid, -1, "{ffffff} Turite atvaþiuoti su tralu!");

	if(VehicleInfo[vehicleid][KiekPrekiuPriekaboje] >= 100)
	{
		VehicleInfo[vehicleid][KasPriekaboje] = modid;
		KraunaMasinaP[playerid] = false;

		format(String, sizeof(String), "Prekës kraunamos ~n~~r~ palaukite apie 30sec", VehicleNames[modid - 400]);
		GameTextForPlayer(playerid, String, 1000, 5);

		format(String, sizeof(String), "{ffffff} {ffffff}Prekës {ffffff}pakrauta á maðinà. Galite vaþiuoti.", VehicleNames[modid - 400]);
		SendClientMessage(playerid, -1, String);

		SetPlayerCheckpointEx 	(playerid, 1, -2161.6792,123.4735,35.3203, 3.0);
		KillTimer				(KraunamPrekesEx[playerid]);
		TogglePlayerControllable(playerid, true);

		return 1;
	}

	VehicleInfo[vehicleid][KiekPrekiuPriekaboje] += 3 + random(3);
	KraunaMasinaP[playerid] = true;

	TogglePlayerControllable(playerid, false);

	format(String, sizeof(String), "Prekës kraunamos~n~ ~r~ palaukite apie 30sec", VehicleNames[modid - 400], VehicleInfo[vehicleid][KiekPrekiuPriekaboje]);
	GameTextForPlayer(playerid, String, 1000, 5);

	return 1;
}
f_public: iskraunamprekesEx(playerid)
{
	new String[128];

	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, -1, "{ffffff} Turite bûti trale!");

	new vehicleid = GetPlayerVehicleID(playerid);

	if(!(KurjeriuMasina[0] <= vehicleid <= KurjeriuMasina[8])) return SendClientMessage(playerid, -1, "{ffffff} Turite atvaþiuoti su tralu!");

	if(VehicleInfo[vehicleid][KiekPrekiuPriekaboje] <= 0)
	{
		IskraunaMasinaP[playerid] = false;

		format(String, sizeof(String), "Iðkraunamos prekes ~r~ palaukite apie 30sec", VehicleNames[VehicleInfo[vehicleid][KasPriekaboje] - 400]);
		GameTextForPlayer(playerid, String, 1000, 5);

		for(new v = 0; v < MAX_VEHICLES; v ++)
		{
			if(ArTurgausMasina[v])
			{
				new model = GetVehicleModel(v);

				switch(model)
				{
					case 401: if(VehicleInfo[vehicleid][KasPriekaboje] == 401) TurgausInfo[v][tKiekis] ++, GivePlayerMoneyA(playerid, 1000);
					case 404: if(VehicleInfo[vehicleid][KasPriekaboje] == 404) TurgausInfo[v][tKiekis] ++, GivePlayerMoneyA(playerid, 1400);
					case 410: if(VehicleInfo[vehicleid][KasPriekaboje] == 410) TurgausInfo[v][tKiekis] ++, GivePlayerMoneyA(playerid, 2000);
					case 412: if(VehicleInfo[vehicleid][KasPriekaboje] == 412) TurgausInfo[v][tKiekis] ++, GivePlayerMoneyA(playerid, 3000);
					case 419: if(VehicleInfo[vehicleid][KasPriekaboje] == 419) TurgausInfo[v][tKiekis] ++, GivePlayerMoneyA(playerid, 4000);
					case 436: if(VehicleInfo[vehicleid][KasPriekaboje] == 436) TurgausInfo[v][tKiekis] ++, GivePlayerMoneyA(playerid, 5000);
				}
		  }
	  }

		format(String, sizeof(String), "{ffffff} {ffffff}Iðkrovëte prekes ir gavote atlygá!", VehicleNames[VehicleInfo[vehicleid][KasPriekaboje] - 400]);
		SendClientMessage(playerid, -1, String);
		GivePlayerMoneyA(playerid, 4500);

		VehicleInfo[vehicleid][KiekPrekiuPriekaboje] 	= 0;
		VehicleInfo[vehicleid][KasPriekaboje] 	= -1;

		KillTimer				(IsKraunamPrekes[playerid]);
		TogglePlayerControllable(playerid, true);

		return 1;
	}

	VehicleInfo[vehicleid][KiekPrekiuPriekaboje] -= 3 + random(3);
	IskraunaMasinaP[playerid] = true;

	TogglePlayerControllable(playerid, false);

	format(String, sizeof(String), "Iðkraunamos prekes~n~ ~r~ palaukite apie 30sec", VehicleNames[VehicleInfo[vehicleid][KasPriekaboje] - 400], VehicleInfo[vehicleid][KiekPrekiuPriekaboje]);
	GameTextForPlayer(playerid, String, 1000, 5);

	return 1;
}
