stock CountPlayerInFaction(faction) // by Johurt
{
	new k = 0;
	for(new p = 0; p < MAX_PLAYERS; p++)
	{
		if(IsPlayerNPC(p) || !IsPlayerConnected(p)) continue;
		if(PI[p][specialybe] == faction)
		{
			k ++;
		}
	}
	return k;
}

